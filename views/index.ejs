<!DOCTYPE html>
<html lang="en">

<%- include("./partials/head.ejs") %> 

<body onfocusout="hide()">
    <%- include("./partials/nav.ejs") %> 

    <div class="main">
        <div class="about">
            <img src="images/exam.svg">
            <div>staff selection commission (SSC) online exam</div>
        </div>
        <div id="login" class="login mdc-elevation--z5">
            <form>
                <div class="mdc-button">
                    <span class="mdc-button__label logintext">LogIn</span>
                </div>
                <div class="ip mdc-text-field mdc-text-field--outlined mdc-text-field--with-leading-icon">
                    <i class="color material-icons mdc-text-field__icon">mail</i>
                    <input id="email" type="email" class=" mdc-text-field__input" id="text-field-hero-input" required>
                    <div class="mdc-notched-outline">
                        <div class="mdc-notched-outline__leading"></div>
                        <div class="mdc-notched-outline__notch">
                            <label for="text-field-hero-input" class="mdc-floating-label">E-mail</label>
                        </div>
                        <div class="mdc-notched-outline__trailing"></div>
                    </div>
                </div>
                <div class="ip mdc-text-field mdc-text-field--outlined mdc-text-field--with-leading-icon">
                    <i class="color material-icons mdc-text-field__icon">lock</i>
                    <input id="password" type="password" minlength="8" maxlength="16" class=" mdc-text-field__input" id="text-field-hero-input" required>
                    <div class="mdc-notched-outline">
                        <div class="mdc-notched-outline__leading"></div>
                        <div class="mdc-notched-outline__notch">
                            <label for="text-field-hero-input" class="mdc-floating-label">Password</label>
                        </div>
                        <div class="mdc-notched-outline__trailing"></div>
                    </div>
                </div>
                <div class="oneline">
                <button type="submit" class="submit padd mdc-button mdc-button--raised">
                    <div class="mdc-button__ripple"></div>
                    <i class="loader material-icons mdc-button__icon" aria-hidden="true">login</i>
                    <span class="mdc-button__label cap">LogIn</span>
                </button>
                <a href="/signup" class="padd mdc-button mdc-button--outlined">
                    <div class="mdc-button__ripple"></div>
                    <i class="material-icons mdc-button__icon" aria-hidden="true"><svg width="18" height="18" viewBox="0 0 18 18" style="fill: currentColor" xmlns="http://www.w3.org/2000/svg">
                        <g clip-path="url(#clip0)">
                        <path d="M5.00003 14V8L3.50003 8V14C3.50003 14.825 4.17503 15.5 5.00003 15.5H15.5C16.325 15.5 17 14.825 17 14V8H15.5V14H5.00003Z" fill="currentColor"/>
                        <path d="M6.50001 3.75L7.55001 4.8L9.50001 2.85V10.5H11V2.85L12.95 4.8L14 3.75L10.25 1.2517e-06L6.50001 3.75Z" fill="currentColor"/>
                        </g>
                        <defs>
                        <clipPath id="clip0">
                        <rect width="18" height="18" fill="white" transform="translate(18) rotate(90)"/>
                        </clipPath>
                        </defs>
                        </svg></i>
                    <span class="mdc-button__label cap">New User? Sign Up</span>
                </a>
            </div>
            <a  href="/reset"class="padd mdc-button">
                <span class="mdc-button__label cap underline">Forgot Password</span>
            </a>
        </form>
        </div>
            
        </div>
        <%- include("./partials/foot.ejs") %> 
</body>
<script src="/index.js"></script>
<script src="/login.js"></script>


</html>